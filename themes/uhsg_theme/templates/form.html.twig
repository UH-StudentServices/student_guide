{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}
{{ attach_library('uhsg_theme/feedback_form') }}
<div class="feedback-form">
	<div class="feedback-form__toggle"> 
		<i class="feedback-form__icon icon--chat icon-2x"></i>
		<span class="feedback-form__title"> {{ 'Feedback'|trans }} </span>
	</div>
	<form{{ attributes.addClass('visually-hidden') }}>
	  {{ children }}
	</form>
</div>
